{
  "texlab.build.executable": "latexmk",
  "texlab.build.args": [
    "-pdf",
    "-bibtex",
    "-interaction=nonstopmode",
    "-synctex=1",
    "-output-directory=out/",
    "%f"
  ],
  "texlab.build.onSave": true,
  "texlab.build.auxDirectory": "out",
  "texlab.build.logDirectory": "out",
  "texlab.build.pdfDirectory": "out"
}
